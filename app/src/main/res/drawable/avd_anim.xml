<animated-vector
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:aapt="http://schemas.android.com/aapt">
	<aapt:attr name="android:drawable">
		<vector
			android:name="vector"
			android:width="20dp"
			android:height="20dp"
			android:viewportWidth="48"
			android:viewportHeight="48">
			<group android:name="group">
				<path
					android:name="path"
					android:fillColor="#00aa87"
					android:pathData="M 39.75 5.89 C 44.99 10.45 48 17.05 48 24 L 39 24 C 39 15.72 32.28 9 24 9 C 19.75 9 15.91 10.77 13.18 13.61 C 10.59 16.3 9 19.97 9 24 C 9 32.28 15.72 39 24 39 C 28.52 39 32.57 37 35.32 33.84 C 38.98 37.02 42.11 39.75 42.11 39.75 C 33.42 49.75 18.26 50.81 8.25 42.11 C 1.34 36.1 -1.3 27 0.6 18.65 C 1.45 14.92 3.2 11.34 5.89 8.25 C 14.58 -1.75 29.74 -2.81 39.75 5.89 Z"
